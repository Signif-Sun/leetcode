#include <stdio.h>
#inlcude <math.h>
  
void main(){

        int m, k;
        puts("please input the number:");
        scanf_s("%d", &m);
        printf("the prime numbers include as following: ");
        for (int j = 1; j <= m; j++) {
                k = sqrt(j);
                for (int i = 2; i <= k+1; i++) {
                        if (j%i == 0) {
                                break;
                        }
                        if (i == k + 1) {
                                printf("%d  ", j);
                        }
                }	
         }
}
